</profile>
<install>
<offering profile='IBM Installation Manager' features='agent_core,agent_jre' id='com.ibm.cic.agent'/>
</install>
<preference value='{{ install_base }}/IM-shared' name='com.ibm.cic.common.core.preferences.eclipseCache'/>
</agent-input>
