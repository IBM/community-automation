<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='{{ install_base }}/{{ shared_dir }}'/>
  </variables>
  <server>
	  <repository location='https://rtpgsa.ibm.com/gsa/rtpgsa/projects/l/liberty.build/liberty/WASX.LIBERTY/{{ liberty_daily_or_test }}/{{ liberty_im_driver }}/linux/zipper/externals/installables/repository/'/>
    <repository location='http://www.ibm.com/software/repositorymanager/com.ibm.java.jdk.v{{ im_java_short_version }}'/>
  </server>
  <profile id='WebSphere Liberty' installLocation='{{ install_base }}/{{ liberty_dir }}'>
    <data key='cic.selector.os' value='{{ cic_selector_os[ansible_system] }}'/>
    <data key='user.feature' value='#NULL'/>
    <data key='user.addon' value='#NULL'/>
    <data key='user.addAssets' value='#NULL'/>
    <data key='user.removeAssets' value='#NULL'/>
    <data key='user.useLibertyRepository' value='false'/>
    <data key='user.accept.license' value='true'/>
    <data key='user.internal.use.only.wlp.install.all.assets' value='com.ibm.websphere.appserver.ndMemberBundle,com.ibm.websphere.appserver.ndControllerBundle'/>
    <data key='user.internal.use.only.wlp.assetsLoaded' value='true'/>
  </profile>
    <!-- IBM WebSphere Application Server Liberty Network Deployment 25.0.0.3 -->
    <offering profile='WebSphere Liberty' id='com.ibm.websphere.liberty.ND' features='liberty'/>
    <!-- IBM Semeru Runtime Certified Edition Version 17 17.0.14.0 -->
    <offering profile='WebSphere Liberty' id='com.ibm.java.jdk.v17' features='com.ibm.sdk.17'/>
  </install>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
</agent-input>
